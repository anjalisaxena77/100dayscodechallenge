class Solution {
    public int lengthOfLastWord(String s) {
        int end=s.length()-1;
    int length=0;
    
    while(end>=0){
        
  if(s.charAt(end)==' '){
        
        if(length>0){
            
            return length;
        }
      
      end--;
        
    }else{
        
        length++;
        end--;
    } 
	
    }
	
    return length;
}
        
    }
